URxvt*cursorBlink:          true
URxvt*cursorUnderline:      true
URxvt*iso14755:             false
URxvt*scrollBar:            false
URxvt*internalBorder:       1

! font settings
URxvt*font:                 xft:Fantasque Sans Mono:style=Regular:pixelsize=10
URxvt*letterSpace:          0

! font rendering
Xft.antialias:              true
Xft.autohint:               0
Xft.dpi:                    96
Xft.hinting:                true
Xft.hintstyle:              hintslight
Xft.lcdfilter:              lcddefault
Xft.rgba:                   rgb

